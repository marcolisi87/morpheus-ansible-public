#!/bin/bash

global
  maxconn 256
  log /dev/log local0 warning
  log-tag container_haproxy_1

defaults
  mode http
  timeout connect 5000ms
  timeout client 50000ms
  timeout server 50000ms
  log global

frontend http-in
  bind *:10000
  default_backend servers

backend servers
  # server server1 127.0.0.1:80 maxconn 32
  